@Article{rieke2018efficacy,
  title = {Efficacy of a Structured Workflow for the Interpretation of Comprehensive Genomic Analysis Data in Clinical Routine},
  author = {DT Rieke and M Lamping and F Klauschen and S Ochsenreither and M Schütte and T Kessler and KF Klinghammer and K Jöhrens and C Messerschmidt and D Lenze and S Burock and D Ditzen and R Schäfer and M Pavel and I Tinhofer and C Sers and D Beule and M-L Yaspo and S Leyvraz and U Keilholz},
  year = {2018},
  first = {F},
  last = {F},
  cited = {1},
  pmid = {NA},
  journal = {J Clin Oncol},
  volume = {36},
  number = {15_suppl},
  pages = {e24164-e24164},
  year = {2018},
  doi = {10.1200/JCO.2018.36.15_suppl.e24164},
  url = { https://doi.org/10.1200/JCO.2018.36.15_suppl.e24164 },
  eprint = { https://doi.org/10.1200/JCO.2018.36.15_suppl.e24164 },
  abstract = { e24164Background: Precision Oncology programs hold promise to improve the treatment of cancer patients. The interpretation and prioritization of complex molecular data from comprehensive analyses has not been standardized. We implemented a knowledge-warehouse-based workflow for sequence data interpretation and treatment recommendations in our molecular tumor board (MTB) and report here the experience in 125 patients. Methods: An interdisciplinary MTB comprising of medical oncology, pathology, bioinformatics and molecular biology was initiated. Molecular analyses including Whole-Exome (WES), targeted and RNA sequencing (RNAseq) were performed within research programs, in clinical routine and by external providers for patients with advanced tumors after standard therapy. A workflow was established for the interpretation of identified aberrations incorporating manual curation, annotation and prioritization based on evidence levels, knowledge databases, IHC validation, in-vivo, in-vitro and in-silico models and interdisciplinary discussions. Results: 125 patients with disease refractory to standard treatments and available molecular analysis of their tumor were discussed in the Charité MTB between 2016 and 2018. WES and RNASeq data were available for 96 patients and panel sequencing for 29. The most common tumor types were melanoma (55), neuroendocrine (33) and colorectal neoplasias (13). All identified aberrations were annotated using prespecified evidence levels with data from a flexible data warehouse and then ranked by the MTB. Recommendations were made for 114 patients with a median number of 2 (range 1-8) recommendations per patient. Most frequent recommendations were MEK, MET, mTOR and PARP-inhibitors. At the time of analyses, 49 patients had started an experimental therapy based on the recommendations. Best responses, as documented, were CR (1), PR (8), MR (3) and SD (7) with missing response data for another N = 7 patients. Conclusions: Implementation of a structured workflow allows for an effective translation of complex molecular data, including WES and RNASeq, into clinical application. },
}

