@Article{hoffmann2022human,
  author = {K Hoffmann and B Obermayer and K Hönzke and D Fatykhova and Z Demir and A Löwa and LGT Alves and E Wyler and E Lopez-Rodriguez and M Mieth and M Baumgardt and J Hoppe and TC Firsching and M Tönnies and TT Bauer and S Eggeling and HL Tran and P Schneider and J Neudecker and JC Rückert and AD Gruber and M Ochs and M Landthaler and D Beule and N Suttorp and S Hippenstiel and AC Hocke and M Kessler},
  title = {Human alveolar progenitors generate dual lineage bronchioalveolar organoids.},
  journal = {Commun Biol},
  year = {2022},
  month = {Aug},
  day = {25},
  volume = {5},
  number = {1},
  pages = {875},
  abstract = {Mechanisms of epithelial renewal in the alveolar compartment remain incompletely understood. To this end, we aimed to characterize alveolar progenitors. Single-cell RNA-sequencing (scRNA-seq) analysis of the HTII-280+/EpCAM+ population from adult human lung revealed subclusters enriched for adult stem cell signature (ASCS) genes. We found that alveolar progenitors in organoid culture in vitro show phenotypic lineage plasticity as they can yield alveolar or bronchial cell-type progeny. The direction of the differentiation is dependent on the presence of the GSK-3$\beta$ inhibitor, CHIR99021. By RNA-seq profiling of GSK-3$\beta$ knockdown organoids we identified additional candidate target genes of the inhibitor, among others FOXM1 and EGF. This gives evidence of Wnt pathway independent regulatory mechanisms of alveolar specification. Following influenza A virus (IAV) infection organoids showed a similar response as lung tissue explants which confirms their suitability for studies of sequelae of pathogen-host interaction.},
  issn = {2399-3642},
  doi = {10.1038/s42003-022-03828-5},
  url = {https://doi.org/10.1038/s42003-022-03828-5},
  cited = {3},
  fulljournalname = {Communications biology},
  issue = {1},
  pmid = {36008580},
  pmcrefcount = {47},
  lastauthor = {Kessler M},
  pmcitedin = {1},
}

